<div class="topbar">
  <div class="container">
    <div class="vertical-center">
      rubyslist > jobs > accounting
    </div>
  </div>
</div>
<div class="container">
  <div id="contact-area">
    <%= form_for @listing do |f| %>
    <%= f.label :タイトル %>
    <%= f.text_field :title %>
    <%= f.label :本文 %>
    <%= f.text_area :description %>
    <%= f.label :都市名 %>
    <%= f.text_field :city %>
    <%= f.label :都道府県 %>
    <%= f.text_field :state %>
    <%= f.label :郵便番号 %>
    <%= f.text_field :zipcode, class: "zip-width" %>
    <%= f.submit class: "create-button" %>
    <% end %>
  </div>
</div>
